{
    "name": "ponyform/sqlite-store-plugin",
    "description": "Official PonyForm Plugin to use SQLite as data store",
    "version": "v0.0.0",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Andreas Mischke",
            "email": "andreas@mischke.me"
        }
    ],
    "require": {
        "ponyform/plugin-contract": "^0.0",
        "respect/validation": "^2.2"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../packages/*"
        },
        {
            "type": "path",
            "url": "../../plugins/*"
        }
    ],
    "autoload": {
        "psr-4": {
            "PonyForm\\SqliteStorePlugin\\": ""
        }
    }
}
